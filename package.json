{
  "name": "betoniera-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run gen-icons && vite --host",
    "build": "npm run clean && npm run gen-icons && tsc && vite build",
    "clean": "npm run clean-dist && npm run clean-icons",
    "clean-dist": "rm -rf ./dist",
    "gen-icons": "pwa-assets-generator",
    "clean-icons": "rm -rf ./public/icons/*.ico && rm -rf ./public/icons/*.png"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.41.0",
    "axios": "^1.6.2",
    "base-64": "^1.0.0",
    "calendar-js": "^1.4.4",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-marquee-text": "^1.0.3",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vite-pwa/assets-generator": "^0.0.11",
    "@vitejs/plugin-react": "^4.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-pwa": "^0.17.2"
  }
}