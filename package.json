{
  "name": "betoniera-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run gen-icons && vite --host",
    "build": "npm run clean && npm run gen-icons && tsc && vite build",
    "clean": "npm run clean-dist && npm run clean-icons",
    "clean-dist": "rm -rf ./dist",
    "gen-icons": "pwa-assets-generator",
    "clean-icons": "rm -rf ./public/icons/*.ico && rm -rf ./public/icons/*.png"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.32.3",
    "axios": "^1.5.1",
    "base-64": "^1.0.0",
    "calendar-js": "^1.4.4",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-clock-emoji": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-easy-swipe": "^0.0.23",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.1",
    "@types/qrcode": "^1.5.2",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vite-pwa/assets-generator": "^0.0.10",
    "@vitejs/plugin-react": "^4.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5"
  }
}
