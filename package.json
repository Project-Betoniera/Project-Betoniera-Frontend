{
  "name": "betoniera-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "build": "npm run clean && npm run generate-icons && npm run generate-licenses && tsc && vite build",
    "clean": "npm run clean-dist && npm run clean-icons",
    "clean-dist": "rimraf ./dist ./dev-dist",
    "clean-licenses": "rimraf -g ./public/licenses ./public/licenses.*",
    "clean-icons": "rimraf -g ./public/icons/*.ico && rimraf -g ./public/icons/*.png",
    "dev": "vite",
    "format": "prettier --write --ignore-unknown .",
    "generate-icons": "pwa-assets-generator",
    "generate-licenses": "node scripts/generate-licenses.js",
    "install": "npm run generate-icons && npm run generate-licenses",
    "start": "npm run dev"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.47.5",
    "axios": "^1.6.8",
    "base-64": "^1.0.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vite-pwa/assets-generator": "^0.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "spdx-expression-parse": "^4.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^0.19.8"
  }
}